<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="inc.faregh.chess_javafx.controll.Genrate_game">
    <stylesheets>
        <URL value="@../styles/Genrate_game.css" />
    </stylesheets>
   <children>
      <HBox alignment="CENTER" layoutY="75.0" prefHeight="17.0" prefWidth="600.0" spacing="10.0">
         <children>
            <Label prefHeight="17.0" prefWidth="30.0" text="5 min" />
            <Slider fx:id="slider" max="60.0" min="5.0" prefHeight="14.0" prefWidth="500.0" value="5.0" />
            <Label text="1 houer" />
         </children>
      </HBox>
      <Label alignment="CENTER" contentDisplay="CENTER" layoutX="189.0" layoutY="129.0" prefHeight="48.0" prefWidth="223.0" text="Theme" textAlignment="RIGHT" />
      <Button fx:id="start" layoutX="189.0" layoutY="329.0" mnemonicParsing="false" onAction="#saveaction" prefHeight="48.0" prefWidth="223.0" text="Start" />
      <Label fx:id="time" alignment="CENTER" contentDisplay="CENTER" layoutX="189.0" layoutY="14.0" prefHeight="48.0" prefWidth="223.0" text="Time: 5" textAlignment="RIGHT" />
      <HBox alignment="CENTER" layoutY="200.0" prefHeight="101.0" prefWidth="600.0" spacing="40.0">
         <children>
            <ToggleButton mnemonicParsing="false" prefHeight="101.0" prefWidth="170.0">
               <toggleGroup>
                  <ToggleGroup fx:id="group" />
               </toggleGroup>
            </ToggleButton>
            <ToggleButton mnemonicParsing="false" prefHeight="101.0" prefWidth="170.0" toggleGroup="$group" />
            <ToggleButton mnemonicParsing="false" prefHeight="101.0" prefWidth="170.0" toggleGroup="$group" />
         </children>
      </HBox>
   </children>
</AnchorPane>
